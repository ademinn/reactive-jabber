%include polycode.fmt
%subst conid a = "\text{\tt {\color{ForestGreen}" a "}}"
%subst varid a = "\text{\tt {\color{Black}" a " }}"
%subst varsym a = "\text{\tt {\color{Plum}" a " }}"
%subst numeral a = "\text{\tt {\color{BurntOrange}" a " }}"
%subst string a = "\text{\tt {\color{OrangeRed} \char34 " a "\char34 }}"
%format qualified = "\mathbf{qualified}"
%format as = "\mathbf{as}"
%format ++ = "\text{\tt {\color{Plum}++}}"
%format $ = "\text{\tt {\color{Plum} \$ }}"